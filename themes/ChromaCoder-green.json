  "tokenColors": [
    {
      "settings": {
        "background": "#00ff00",
        "foreground": "#ffffff"
      }
    },

    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#880088"
      }
    },

    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#8844ff"
      }
    },

    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#6666ff"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#6666ff"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#6666ff"
      }
    },

    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#ffffff"
      }
    },

    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ff0066"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#ff0066"
      }
    },

    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#00aaff"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.type, entity.name.class",
      "settings": {
        "foreground": "#88ddff"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#88ddff"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#88ddff"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ffaaff"
      }
    },

    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff0066"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#88ddff"
      }
    },

    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#00aaff"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#00aaff"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "#00aaff"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
      }
    },

    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#ff0066",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#6666ff",
        "foreground": "#ffffff"
      }
    },

    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#8844ff"
      }
    },

    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#550055"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ff0066"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#88ddff"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#8844ff"
      }
    },

    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#6666ffA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#8844ff"
      }
    },

    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8844ff"
      }
    },
    {
      "name": "Markdown Code",
      "scope": "markup.inline.raw.markdown, markup.raw.block.markdown, markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#ffaaff"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88ddff"
      }
    },
    {
      "name": "Markdown List",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "#00aaff"
      }
    },
    {
      "name": "Markdown Header",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff0066"
      }
    },
    {
      "name": "Markdown Link",
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#6666ff"
      }
    },
    {
      "name": "Markdown Image",
      "scope": "meta.image.inline.markdown",
      "settings": {
        "foreground": "#6666ff"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#550055"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#6666ff"
      }
    }
  ]
}